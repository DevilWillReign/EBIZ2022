# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /crud1                    controllers.Crud1Controller.showAll()
POST    /crud1                    controllers.Crud1Controller.add()
PUT     /crud1/:id                    controllers.Crud1Controller.update(id: Int)
GET     /crud1/:id                    controllers.Crud1Controller.showById(id: Int)
DELETE  /crud1/:id                    controllers.Crud1Controller.delete(id: Int)

GET     /crud2                    controllers.Crud2Controller.showAll()
POST    /crud2                    controllers.Crud2Controller.add()
PUT     /crud2/:id                    controllers.Crud2Controller.update(id: Int)
GET     /crud2/:id                    controllers.Crud2Controller.showById(id: Int)
DELETE  /crud2/:id                    controllers.Crud2Controller.delete(id: Int)

GET     /crud3                    controllers.Crud3Controller.showAll()
POST    /crud3                    controllers.Crud3Controller.add()
PUT     /crud3/:id                    controllers.Crud3Controller.update(id: Int)
GET     /crud3/:id                    controllers.Crud3Controller.showById(id: Int)
DELETE  /crud3/:id                    controllers.Crud3Controller.delete(id: Int)

